{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "althinect/filament-spatie-roles-permissions": "^2.2",
        "bezhansalleh/filament-shield": "^3.2",
        "eightynine/filament-excel-import": "^3.0",
        "filament/actions": "^3.2",
        "filament/filament": "^3.2",
        "filament/tables": "^3.2",
        "guzzlehttp/guzzle": "^7.2",
        "joaopaulolndev/filament-edit-profile": "^1.0",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.3",
        "laravel/tinker": "^2.8",
        "mckenziearts/laravel-notify": "^2.4",
        "nativephp/electron": "^0.6.3",
        "outerweb/filament-settings": "^1.1",
        "picqer/php-barcode-generator": "^2.4",
        "psr/simple-cache": "2.0",
        "pxlrbt/filament-excel": "^2.3",
        "stechstudio/filament-impersonate": "^3.8",
        "tomatophp/filament-users": "^1.0",
        "usernotnull/tall-toasts": "^2.0",
        "yoeunes/toastr": "^2.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Services\\": "services/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
